{
    "name": "rgilyov/laravel-csv-importer",
    "description": "Easy and reliable way to import, parse, validate and transform your csv files with laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["csv", "import", "laravel", "parse", "validate", "transform"],
    "authors": [
        {
            "name": "Roman Gilyov",
            "email": "rgilyov@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "5.7",
        "mockery/mockery": "^0.9.4",
        "predis/predis": "1.0.*"
    },
    "require": {
        "php": ">=5.6.4",
        "arvenil/ninja-mutex": "0.6.0",
        "league/csv": "8.0",
        "laravel/framework": "~4.2|^5",
        "nesbot/carbon": "~1.20"
    },
    "autoload": {
        "psr-4": {
            "RGilyov\\CsvImporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RGilyov\\CsvImporter\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
